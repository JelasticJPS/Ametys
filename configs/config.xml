  <!-- plugin.web:PLUGINS_WEB_CONFIG_CACHE_MONITORING_GROUP -->
  <cache.monitoring.datasource.jdbc.pool>SQL-default-datasource</cache.monitoring.datasource.jdbc.pool>
  <cache.monitoring.schedulers.enable>false</cache.monitoring.schedulers.enable>

