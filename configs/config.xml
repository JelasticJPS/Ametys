  <store.external.files>false</store.external.files>
  <store.files.extensions>pdf,zip,gz,bz2,7z,rar,doc,docx,xls,xlsx,ppt,pptx,odt</store.files.extensions>
  <!-- plugin.cms:CONTENT_ARCHIVE_CONFIG_GROUP -->
  <archive.scheduler.hour>01:00</archive.scheduler.hour>
  <archive.scheduler.enabled>false</archive.scheduler.enabled>
  <archive.scheduler.reminder.delay>7</archive.scheduler.reminder.delay>
