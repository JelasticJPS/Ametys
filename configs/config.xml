      | plugin.repository:PLUGINS_REPOSITORY_CONFIG_CATEGORY
      +-->

  <!-- plugin.cms:PLUGINS_CMS_PURGE_CONTENTS_CONFIG_GROUP -->
  <purge.scheduler.period>7</purge.scheduler.period>
  <purge.scheduler.enabled>false</purge.scheduler.enabled>
  <purge.before.days>60</purge.before.days>
  <purge.keep.validated.versions>3</purge.keep.validated.versions>
  <purge.scheduler.hour>02:00</purge.scheduler.hour>
