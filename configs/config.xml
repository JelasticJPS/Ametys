  <remind.before.publication.end>7</remind.before.publication.end>
  <remind.unmodified.content.delay>30</remind.unmodified.content.delay>
  <remind.content.validation.delay>10</remind.content.validation.delay>
  <remind.content.enabled>false</remind.content.enabled>
  <alerts.scheduler.hour>02:00</alerts.scheduler.hour>
  <!-- plugin.cms:PLUGINS_CMS_GLOBAL_CONTENT_CONSISTENCY_CONFIG_GROUP -->
  <consistency.scheduler.enabled>true</consistency.scheduler.enabled>
  <consistency.scheduler.hour>02:00</consistency.scheduler.hour>
